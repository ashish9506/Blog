  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug organisationStats.js",
      "program": "${workspaceFolder}/scripts/organisationStats.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node",
      "env": {
        "NODE_ENV": "default"
      }
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Debug ParkOne API",
      "skipFiles": ["<node_internals>/**"],
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ],
      "program": "${workspaceFolder}/index.js",
      "outFiles": ["${workspaceFolder}/**/*.js"],
      "env": {
        "NODE_ENV": "default"
      }
    }
  ]
}
